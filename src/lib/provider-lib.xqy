declare function m:lookup-abbrev($code as xs:string) as xs:string?
{
  (map:get($ABBREVIATIONS, $code), $code)[1]
};

declare function m:make-address-str($element)
{
  fn:string-join(
    (
      $element/cms:addr-line1[. ne '']/fn:string(),
      $element/cms:addr-line2[. ne '']/fn:string(),
      $element/cms:addr-line3[. ne '']/fn:string(),
      $element/cms:city[. ne '']/fn:string(),
      $element/cms:state[. ne '']/fn:string() || " " || $element/cms:zip5[. ne '']/fn:string()
    ),
    ", ")
};

declare function m:zip-geo(
  $element,
  $zip as xs:string?)
{
  let $zip-info :=
    if ($zip) then
      /zip-geo:zip_geo[zip-geo:postal = $zip]
    else ()
  return
    if ($zip-info) then
    element { fn:node-name($element) } {
        $element/@*,
        attribute lat { $zip-info/zip-geo:latitude/fn:data(.) },
        attribute lng { $zip-info/zip-geo:longitude/fn:data(.) },
        $element/node()
      }
    else
      $element
};

declare function m:geocode($element, $addr-str)
{
  let $loc := geo:geocode($addr-str)
  return
    if ($loc and $loc/@lat) then
      element { fn:node-name($element) } {
        $element/@*,
        attribute lat { $loc/@lat },
        attribute lng { $loc/@lng },
        $element/node()
      }
    else
      $element
};

declare function m:split-zip($zip-element as element()?)
{
  let $zip := fn:string($zip-element)
  return
    if (fn:string-length($zip) = 9) then
    (
      element cms:zip5 { fn:substring($zip, 1, 5) },
      element cms:zip4 { fn:substring($zip, 6, 4) }
    )
    else if (fn:string-length($zip) = 5) then
      element cms:zip5 { $zip }
    else
      ()
};

declare function m:normalize-address($addr as xs:string?)
{
  let $toks as xs:string* :=
    for $tok in fn:tokenize($addr, " ")
    let $tok := fn:replace($tok, "[.#,]", "")
    return
      m:lookup-abbrev($tok)
  return
    fn:string-join($toks, " ")
};

declare function m:normalize-credentials($creds as xs:string?)
{
  fn:replace($creds, "\.", "")[. != '']
};

declare function m:parse-date($date as xs:string)
{
  fn:replace($date, "(\d\d)/(\d\d)/(\d\d\d\d)", "$3-$1-$2")
};


declare function m:build-profile($provider as element(provider))
{
  let $id := $provider/NPI/fn:data(.)
  let $entity-type-code as xs:string? := $provider/*:Entity_Type_Code
  where $entity-type-code[. ne '']
  return
    <provider xmlns="http://marklogic.com/cms">
    {
      element npi { $id },
      $provider/*:Employer_Identification_Number__EIN_[. ne '']/element ein { fn:data(.) },

      element entity-type {
        attribute code { $entity-type-code },
        map:get($ENTITY-TYPE-CODES, $entity-type-code)
      },
      if ($entity-type-code = "1") then
