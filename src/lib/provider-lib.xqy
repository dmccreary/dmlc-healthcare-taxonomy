           },

        let $state as xs:string? := $provider/*:Other_Provider_Identifier_State_2[. ne '']
        let $type-code as xs:string? := $provider/*:Other_Provider_Identifier_Type_Code_2[. ne '']
        let $issuer as xs:string? := $provider/*:Other_Provider_Identifier_Issuer_2[. ne '']
        let $value as xs:string? := $provider/*:Other_Provider_Identifier_2[. ne '']
        where ($state or $type-code or $issuer or $value)
        return
